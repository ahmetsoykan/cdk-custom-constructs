{
    "name": "cdk-github",
    "version": "0.0.0",
    "description": "CDK Construct for Github Resources",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "jsii",
        "build:watch": "jsii -w",
        "package": "jsii-pacmak",
        "test": "tsc && jest",
        "watch": "tsc -w"
    },
    "jsii": {
        "outdir": "dist",
        "targets": {
            "python": {
                "distName": "cdk-github",
                "module": "github"
            }
        }
    },
    "license": "ISC",
    "devDependencies": {
        "@aws-cdk/assert": "1.91.0",
        "@types/jest": "^26.0.20",
        "@types/node": "14.14.16",
        "aws-sdk": "^2.817.0",
        "jest": "^26.6.3",
        "jsii": "^1.16.0",
        "jsii-pacmak": "^1.16.0",
        "ts-jest": "^26.5.2",
        "ts-node": "^9.1.1",
        "typescript": "~4.1.3"
    },
    "dependencies": {
        "@aws-cdk/core": "1.91.0",
        "constructs": "^3.3.41"
    },
    "peerDependencies": {
        "@aws-cdk/core": "1.91.0",
        "constructs": "^3.3.41"
    }
}
